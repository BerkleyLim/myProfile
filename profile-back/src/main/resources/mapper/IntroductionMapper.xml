<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
    PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    
<mapper namespace="profile.back.mapper.IntroductionMapper">
  <resultMap type="Introduction" id="IntroductionMap">
	  <id column="tno" property="no"/>
	  <result column="title" property="title"/>
	  <result column="contents" property="contents"/>
	  <result column="view" property="view"/>
  </resultMap>
  
  <select id="findById" resultType="Introduction">
    select * from Together where id = #{id}
  </select>
</mapper>